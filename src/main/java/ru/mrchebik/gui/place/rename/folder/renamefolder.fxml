<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="ru.mrchebik.gui.place.rename.folder.RenameFolderPresenter">
    <GridPane layoutX="90.0" layoutY="67.0" prefHeight="100.0" prefWidth="400.0" AnchorPane.bottomAnchor="30.0"
              AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.0" minWidth="10.0" prefWidth="57.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="341.0" minWidth="10.0" prefWidth="182.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="341.0" minWidth="10.0" prefWidth="106.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints maxHeight="68.0" minHeight="36.0" prefHeight="36.0"/>
        </rowConstraints>
        <Label text="Name:"/>
        <TextField fx:id="name" onKeyPressed="#handleRenameWithKey" prefHeight="26.0" prefWidth="276.0"
                   GridPane.columnIndex="1" GridPane.columnSpan="2"/>
        <VBox alignment="BOTTOM_RIGHT" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2"
              GridPane.rowIndex="1">
            <Button mnemonicParsing="false" onMouseClicked="#handleRename" text="Rename"/>
        </VBox>
    </GridPane>
</AnchorPane>
